ayout/Page.tsx + 1 modules\nvar Page = __webpack_require__(97);\n\n// CONCATENATED MODULE: ./src/components/PageLoader.tsx\nvar PageLoader_templateObject,_templateObject2;var Wrapper=Object(styled_components_browser_esm[\"e\" /* default */])(Page[\"a\" /* default */])(PageLoader_templateObject||(PageLoader_templateObject=Object(taggedTemplateLiteral[\"a\" /* default */])([\"\\n  display: flex;\\n  justify-content: center;\\n  align-items: center;\\n\"])));var LogoIcon=styled_components_browser_esm[\"e\" /* default */].div(_templateObject2||(_templateObject2=Object(taggedTemplateLiteral[\"a\" /* default */])([\"\\n  transition: transform 0.3s ease;\\n  margin-bottom: 24px;\\n  animation: pulse 1.25s ease-in-out infinite;\\n  @keyframes pulse {\\n    0% {\\n      -webkit-transform: scaleX(1);\\n      transform: scaleX(1);\\n    }\\n    50% {\\n      -webkit-transform: scale3d(1.05, 1.05, 1.05);\\n      transform: scale3d(1.05, 1.05, 1.05);\\n    }\\n    to {\\n      -webkit-transform: scaleX(1);\\n      transform: scaleX(1);\\n    }\\n  }\\n\"])));var PageLoader_PageLoader=function PageLoader(){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Wrapper,{children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(LogoIcon,{children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"img\",{style:{height:86},src:\"https://www.smurf.money/images/blzd/logo.png\",alt:\"logo\"})})});};/* harmony default export */ var components_PageLoader = (PageLoader_PageLoader);\n// EXTERNAL MODULE: ./node_modules/lodash/orderBy.js\nvar orderBy = __webpack_require__(204);\nvar orderBy_default = /*#__PURE__*/__webpack_require__.n(orderBy);\n\n// EXTERNAL MODULE: ./node_modules/lodash/partition.js\nvar partition = __webpack_require__(331);\nvar partition_default = /*#__PURE__*/__webpack_require__.n(partition);\n\n// EXTERNAL MODULE: ./src/hooks/useI18n.ts + 1 modules\nvar useI18n = __webpack_require__(29);\n\n// EXTERNAL MODULE: ./src/components/layout/Flex.tsx\nvar Flex = __webpack_require__(318);\n\n// EXTE